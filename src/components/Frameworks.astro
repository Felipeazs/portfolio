---
import react from "../assets/react.png"
import express from "../assets/express.png"
import hono from "../assets/hono.png"
import tailwind from "../assets/tailwind.png"
import astro from "../assets/astro.png"
import spring_boot from "../assets/spring_boot.png"
import svelte from "../assets/svelte.png"

import Card from "./Card.astro"

const width = 50
const height = 50

const images = [
	{
		src: react.src,
		width,
		height,
		alt: "react",
	},
	{
		src: express.src,
		width,
		height,
		alt: "express",
	},
	{
		src: hono.src,
		width,
		height,
		alt: "hono",
	},
	{
		src: tailwind.src,
		width,
		height,
		alt: "tailwind",
	},
	{
		src: astro.src,
		width,
		height,
		alt: "astro",
	},
	{
		src: spring_boot.src,
		width,
		height,
		alt: "spring boot",
	},
	{
		src: svelte.src,
		width,
		height,
		alt: "svelte",
	},
]
---

<Card
	title="Frameworks"
	images={images}
/>
