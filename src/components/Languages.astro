---
import js from "../assets/js.png"
import ts from "../assets/ts.png"
import python from "../assets/python.png"
import java from "../assets/java.png"

import Card from "./Card.astro"

const width = 50
const height = 50

const images = [
	{
		src: js.src,
		width,
		height,
		alt: "javascript",
	},
	{
		src: ts.src,
		width,
		height,
		alt: "typescript",
	},
	{
		src: python.src,
		width,
		height,
		alt: "python",
	},
	{
		src: java.src,
		width,
		height,
		alt: "java",
	},
]
---

<Card
	title="Lenguages"
	images={images}
/>
