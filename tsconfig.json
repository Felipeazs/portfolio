{
    "extends": "astro/tsconfigs/base",
    "compilerOptions": {
        "jsx": "react-jsx",
        "jsxImportSource": "react",
        "strictNullChecks": true,
        "allowJs": true,
        "plugins": [
            {
                "name": "@astrojs/ts-plugin"
            }
        ],
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./src/*"
            ]
        }
    },
    "include": [
        ".astro/types.d.ts",
        "**/*"
    ],
    "exclude": [
        "dist"
    ]
}
